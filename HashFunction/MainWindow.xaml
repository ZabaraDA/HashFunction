<Window x:Class="HashFunction.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashFunction"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox Width="250" Height="40" Name="HashTextBox"/>
            <TextBox Width="250" Height="40" TextAlignment="Center" Name="HashTextBlock"/>
            <Button Width="150" Height="40" Margin="10" Click="Button_Click"/>
            <Button Width="150" Height="40" Margin="10" Click="Button_Click_1"/>
        </StackPanel>
    </Grid>
</Window>
